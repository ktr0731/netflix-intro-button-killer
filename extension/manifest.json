{
  "manifest_version": 2,

  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "ja",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon16.png"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/watch/*"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ]
}
